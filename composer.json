{
  "name": "mouf/codestylefixservice",
  "description": "Fix code style automatically",
  "keyword": ["mouf", "code style", "php", "fixer"],
  "homepage": "http://mouf-php.com/packages/mouf/codestylefixservice",
  "type": "mouf-library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jean-Baptiste Charron",
      "email": "jb.charron@thecodingmachine.com"
    }
  ],
  "autoload": {
    "psr-4": {"Mouf\\FixService\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": {"Mouf\\FixService\\": "tests/"}
  },
  "require": {
    "fabpot/php-cs-fixer": "~1.11"
  },
  "require-dev" : {
    "phpunit/phpunit": "~4.0 | ~5.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}